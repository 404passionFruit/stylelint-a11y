{
  "name": "stylelint-a11y",
  "version": "1.0.0",
  "description": "Plugin for stylelint with a11y rules",
  "main": "dist/index.js",
  "repository": "https://github.com/YozhikM/stylelint-a11y.git",
  "author": "Stanislav <botev.st@gmail.com>",
  "license": "MIT",
  "keywords": [
    "stylelint",
    "stylelint-plugin",
    "a11y"
  ],
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "precommit": "lint-staged",
    "prebuild": "rimraf dist",
    "build": "tsc --emitDeclarationOnly && babel src --out-dir lib --extensions \".ts,.tsx\"",
    "oldbuild": "babel src --out-dir dist",
    "lint": "eslint . --ignore-path .gitignore",
    "prettify": "prettier --write src/**/*",
    "prepublish": "yarn build",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "**/*.{js,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "stylelint": "^8.0.0 || ^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/node": "^10.3.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-jest": "^23.0.1",
    "babel-preset-typescript": "^7.0.0-alpha.19",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-stylelint": "^8.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^2.6.0",
    "jest": "^23.1.0",
    "jest-cli": "^23.1.0",
    "lodash": "^4.17.10",
    "postcss": "^6.0.22",
    "prettier": "^1.13.4",
    "rimraf": "^2.6.2",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-test-rule-tape": "^0.2.0",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.1"
  }
}
